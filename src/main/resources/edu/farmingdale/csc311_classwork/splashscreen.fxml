<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.farmingdale.csc311_classwork.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label text="Welcome To">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <ImageView fx:id="fsc" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#onFscClick" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/fsc.png" />
         </image>
      </ImageView>
      <Slider fx:id="splashSlider" onMouseClicked="#onSliderClick" />
   </children>
</VBox>
